    "license": "MIT",
    "main": "main.js",
    "scripts": {
        "chore": "npm install",
        "dev": "npm run tailwind:watch & node esbuild.config.mjs",
        "build": "npm run tailwind:build && tsc --noEmit --skipLibCheck && node esbuild.config.mjs release",
        "tailwind:watch": "tailwindcss -i ./src/tailwind-input.css -o ./src/tailwind.css --watch",
        "tailwind:build": "tailwindcss -i ./src/tailwind-input.css -o ./src/tailwind.css --minify"
    },
    "devDependencies": {
        "@types/vue-markdown": "^2.2.4",
        "autoprefixer": "^10.4.23",
        "builtin-modules": "^3.3.0",
        "esbuild": "^0.18.17",
        "esbuild-plugin-vue3": "^0.3.2",
        "obsidian": "^1.3.5",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.19",
        "typescript": "^5.1.6",
        "vue": "^3.3.4"
    },
    "dependencies": {
        "babel-runtime": "^6.26.0",
        "d3": "^7.9.0",
        "element-plus": "^2.9.7",
        "highlight.js": "^11.11.1",
        "katex": "^0.16.21",
        "mathjax-full": "^3.2.2",
        "mermaid": "^10.9.1",
        "openai": "^4.91.1",
        "pinia": "^3.0.1",
        "segmentit": "^2.0.3",
        "wordcloud": "^1.2.3"
    }
